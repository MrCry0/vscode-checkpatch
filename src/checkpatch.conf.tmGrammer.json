{
    "name": "checkpatch.config",
    "scopeName": "text.checkpatch",
    "fileTypes": [
        "checkpatch.config"
    ],
    "patterns": [
        {
            "include": "#lineComment"
        },
        {
            "include": "#flag"
        },
        {
            "include": "#flagType"
        },
        {
            "include": "#flagNum"
        },
        {
            "include": "#flagOther"
        }
    ],
    "repository": {
        "lineComment": {
            "name": "comment.line.text.checkpatch",
            "comment": "Comments",
            "match": "#.*$"
        },
        "flag": {
            "name": "keyword.text.checkpatch",
            "match": "(--(?:q|quiet|no-tree|no-signoff|emacs|terse|showfile|subjective|strict|mailback|ignore-perl-version|codespell))$"
        },
        "flagType": {
            "name": "meta.repository.text.checkpatch",
            "match": "(--(?:ignore|types))(.*)$",
            "captures": {
                "1": { "name": "keyword.text.checkpatch" },
                "2": { "patterns": [ { "include": "#type" } ] }
            }
        },
        "type": {
            "name": "entity.name.type.text.checkpatch",
            "match": "(ALLOC_ARRAY_ARGS|ALLOC_SIZEOF_STRUCT|ALLOC_WITH_MULTIPLY|ARCH_DEFINES|ARCH_INCLUDE_LINUX|ARRAY_SIZE|ASSIGNMENT_CONTINUATIONS|ASSIGN_IN_IF|AVOID_BUG|AVOID_EXTERNS|BAD_SIGN_OFF|BIT_MACRO|BLOCK_COMMENT_STYLE|BOOL_COMPARISON|BRACES|BRACKET_SPACE|C99_COMMENTS|CAMELCASE|CODE_INDENT|COMMIT_LOG_LONG_LINE|COMMIT_MESSAGE|COMPARISON_TO_NULL|COMPLEX_MACRO|CONCATENATED_STRING|CONFIG_DESCRIPTION|CONFIG_TYPE_BOOLEAN|CONSIDER_COMPLETION|CONSIDER_KSTRTO|CONSTANT_COMPARISON|CONSTANT_CONVERSION|CONST_CONST|CONST_READ_MOSTLY|CONST_STRUCT|CORRUPTED_PATCH|CVS_KEYWORD|DATE_TIME|DEEP_INDENTATION|DEFAULT_NO_BREAK|DEFINE_ARCH_HAS|DEPRECATED_API|DEPRECATED_VARIABLE|DEVICE_ATTR_FUNCTIONS|DEVICE_ATTR_PERMS|DEVICE_ATTR_RO|DEVICE_ATTR_RW|DEVICE_ATTR_WO|DIFF_IN_COMMIT_MSG|DOS_LINE_ENDINGS|DO_WHILE_MACRO_WITH_TRAILING_SEMICOLON|DT_SPLIT_BINDING_PATCH|DUPLICATED_SYSCTL_CONST|ELSE_AFTER_BRACE|EMAIL_SUBJECT|EMBEDDED_FUNCTION_NAME|ENOSYS|EXECUTE_PERMISSIONS|EXPORTED_WORLD_WRITABLE|EXPORT_SYMBOL|FILE_PATH_CHANGES|FSF_MAILING_ADDRESS|FUNCTION_ARGUMENTS|FUNCTION_WITHOUT_ARGS|GERRIT_CHANGE_ID|GIT_COMMIT_ID|GLOBAL_INITIALISERS|HEXADECIMAL_BOOLEAN_TEST|HOTPLUG_SECTION|IF_0|IF_1|INCLUDE_LINUX|INDENTED_LABEL|INITIALISED_STATIC|INIT_ATTRIBUTE|INLINE|INLINE_LOCATION|INVALID_UTF8|IN_ATOMIC|JIFFIES_COMPARISON|KREALLOC_ARG_REUSE|LEADING_SPACE|LIKELY_MISUSE|LINE_CONTINUATIONS|LINE_SPACING|LINUX_VERSION_CODE|LOCKDEP|LOCKING|LOGGING_CONTINUATION|LOGICAL_CONTINUATIONS|LONG_LINE|LONG_LINE_COMMENT|LONG_LINE_STRING|LONG_UDELAY|MACRO_ARG_PRECEDENCE|MACRO_ARG_REUSE|MACRO_WITH_FLOW_CONTROL|MAINTAINERS_STYLE|MALFORMED_INCLUDE|MASK_THEN_SHIFT|MEMORY_BARRIER|MEMSET|MINMAX|MISORDERED_TYPE|MISPLACED_INIT|MISSING_BREAK|MISSING_EOF_NEWLINE|MISSING_SIGN_OFF|MODIFIED_INCLUDE_ASM|MODULE_LICENSE|MSLEEP|MULTILINE_DEREFERENCE|MULTIPLE_ASSIGNMENTS|MULTIPLE_DECLARATION|MULTISTATEMENT_MACRO_USE_DO_WHILE|NAKED_SSCANF|NETWORKING_BLOCK_COMMENT_STYLE|NEW_TYPEDEFS|NON_OCTAL_PERMISSIONS|NOT_UNIFIED_DIFF|NO_AUTHOR_SIGN_OFF|NR_CPUS|OBSOLETE|ONE_SEMICOLON|OOM_MESSAGE|OPEN_BRACE|OPEN_ENDED_LINE|PARENTHESIS_ALIGNMENT|PATCH_PREFIX|POINTER_LOCATION|PREFER_ALIGNED|PREFER_DEV_LEVEL|PREFER_ETHER_ADDR_COPY|PREFER_ETHER_ADDR_EQUAL|PREFER_ETH_BROADCAST_ADDR|PREFER_ETH_ZERO_ADDR|PREFER_IS_ENABLED|PREFER_KERNEL_TYPES|PREFER_PACKED|PREFER_PRINTF|PREFER_PR_LEVEL|PREFER_SCANF|PREFER_SEQ_PUTS|PRINTF_0XDECIMAL|PRINTF_L|PRINTF_Z|PRINTK_RATELIMITED|PRINTK_WITHOUT_KERN_LEVEL|QUOTED_WHITESPACE_BEFORE_NEWLINE|READ_BARRIER_DEPENDS|RETURN_PARENTHESES|RETURN_VOID|SINGLE_STATEMENT_DO_WHILE_MACRO|SIZEOF_ADDRESS|SIZEOF_PARENTHESIS|SPACE_BEFORE_TAB|SPACING|SPDX_LICENSE_TAG|SPLIT_STRING|SSCANF_TO_KSTRTO|STATIC_CONST_CHAR_ARRAY|STORAGE_CLASS|STRING_FRAGMENTS|SUSPECT_CODE_INDENT|SWITCH_CASE_INDENT_LEVEL|SYMBOLIC_PERMS|TABSTOP|TEST_ATTR|TEST_NOT_ATTR|TEST_NOT_TYPE|TEST_TYPE|TRAILING_SEMICOLON|TRAILING_STATEMENTS|TRAILING_WHITESPACE|TYPECAST_INT_CONSTANT|TYPO_SPELLING|UAPI_INCLUDE|UNCOMMENTED_DEFINITION|UNDOCUMENTED_DT_STRING|UNDOCUMENTED_SETUP|UNNECESSARY_BREAK|UNNECESSARY_CASTS|UNNECESSARY_ELSE|UNNECESSARY_INT|UNNECESSARY_KERN_LEVEL|UNNECESSARY_PARENTHESES|UNSPECIFIED_INT|USE_DEVICE_INITCALL|USE_FUNC|USE_LOCKDEP|USE_NEGATIVE_ERRNO|USE_RELATIVE_PATH|USE_SPINLOCK_T|USLEEP_RANGE|UTF8_BEFORE_PATCH|VOLATILE|VSPRINTF_POINTER_EXTENSION|VSPRINTF_SPECIFIER_PX|WAITQUEUE_ACTIVE|WEAK_DECLARATION|WHILE_AFTER_BRACE|WHITESPACE_AFTER_LINE_CONTINUATION|YIELD)"
        },
        "flagNum": {
            "name": "meta.repository.text.checkpatch",
            "match": "(--(?:max-line-length|min-conf-desc-length))(.*)$",
            "captures": {
                "1": { "name": "keyword.text.checkpatch" },
                "2": { "patterns": [ { "include": "#num" } ] }
            }
        },
        "num": {
            "name": "constant.numeric.text.checkpatch",
            "match": "(\\d+)"
        },
        "flagOther": {
            "name": "meta.repository.text.checkpatch",
            "match": "(--(?:root|codespellfile))(.*)$",
            "captures": {
                "1": { "name": "keyword.text.checkpatch" }
            }
        }
    }
}
